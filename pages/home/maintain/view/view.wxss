/* pages/home/maintain/view/view.wxss */
.container{
  padding-bottom: 53px;
}
.price .center{
  flex-direction: row;
  justify-content: flex-start;
}
.price .center text{
  margin-right: 20rpx;
}
.old_price{
  text-decoration:  line-through;
}
.tag text{
  margin-right: 20rpx;
}
.parameter{
  padding: 2% 0;
  margin-bottom: 20rpx;
  background: #fff;
  border-top: 1rpx solid #f2f2f2;
   border-bottom: 1rpx solid #f2f2f2;
}
.cont{
  padding: 0 4%;
  line-height: 1.6em;
}
.cont image{
  width: 100%;
}
.flex .flex-item.buy_btn {
    -webkit-box-flex: 2;
    -webkit-flex: 2;
    flex: 2;
    
    color: #fff;
    background: #febf00;
}
.collect{
  background: #e9e9e9;
}
.bottom{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  line-height: 53px;
  background: #fff;
}