/* pages/discount/discount.wxss */

.container {
  background: #fff;
}

.list {
  padding: 4%;
}

.list .list_item {
  display: inline-block;
  width: 48%;
  border: 1px solid #cdcdcd;
  position: relative;
  margin: 0 0 2% 0;
}

.list .list_item:nth-child(2n+1) {
  margin: 0 2% 2% 0;
}

.list .list_item image {
  width: 100%;
}

.item_txt {
  padding: 2%;
}

.item_txt>view {
  margin-bottom: 10rpx;
}

.price_btn {
  padding: 8rpx 12rpx;
  border-radius: 10rpx;
}

.tag {
  position: absolute;
  top: 0;
  left: 0;
  padding: 5rpx 15rpx;
  background: #ff9501;
}

.search {
  padding: 4% 0 0;
  background-size: 100% 90%;
  background: url("data:image/png;base64,");
}

.search_frame {
  background: #fff;
  box-shadow: 0 0 8px #9ed3f7;
  z-index: 999;
  margin-top: 10px;
  padding: 5px 0 5px 5px;
  border-radius: 3px;
  width: 94%;
  margin: 0 auto;
  position: relative;
}

.search_frame .iconfont{
  position: absolute;
  top: 20rpx;
  left: 10rpx;
}
.search_frame input{
  padding-left: 50rpx;
  height: 50rpx !important;
  min-height: 50rpx !important;
}

.history {
  width: 90%;
  padding: 2vw;
  margin: 4vw auto;
  background: #ff9501;
  border-radius: 5px;
}

.history_cont text {
  line-height: 2;
  display: inline-block;
  padding: 0 15rpx;
  margin: 2vw 2% 0 0;
  border-radius: 3px;
  background: #fff;
}

.history_cont text.active {
  color: #0f84de;
  background: #e5f4ff;
}
