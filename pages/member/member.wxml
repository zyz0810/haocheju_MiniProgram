<view class="top">
  <view class='lineItem clr-white'>
    <view class='left'>
    <image src='http://m.0556360.com/weixin/images/placeholder/logo.png'></image>
    <text class='grade clr-white'>v</text>
    </view>
    <view class='center'>
      <view class='f16'>好车居</view>
      <view class='f14'>just do it!</view>
    </view>
    <view class='right' bindtap='goEdit'><text class='iconfont icon-icon-edit f20 white'></text></view>
  </view>
</view>
<view class='order'>
  <view class='lineItem border_bottom' data-current="0" bindtap='goOrder'>
    <view class='center f15'>我的订单</view>
    <view class='right f12 clr_lightGray'>查看全部订单</view>
  </view>
  <view class='flex clr_mediumGray' data-current="1">
    <view class='flex-item tc' bindtap='goOrder'>
      <text class='iconfont icon-daifukuan f18'></text>
      <view class='f12'>代付款</view>
    </view>
    <view class='flex-item tc' data-current="2" bindtap='goOrder'>
      <text class='iconfont icon-daishouhuo'></text>
      <view class='f12'>代收货</view>
    </view>
    <view class='flex-item tc' data-current="3" bindtap='goOrder'>
      <text class='iconfont icon-weishiyongyouhuiquan'></text>
      <view class='f12'>未使用</view>
    </view>
  </view>
</view>
<view class='menu clr_mediumGray'>
  <view class='menu_item f12 tc' bindtap='goInsurance'>
    <view class='iconfont icon-baoxian f24'></view>
    <view>我的保险</view>
  </view>
   <view class='menu_item f12 tc' bindtap='goCar'>
   <view class='iconfont icon-zhengchekucun- f24'></view>
    <view>我的车库</view>
  </view>
   <view class='menu_item f12 tc' bindtap='goPersonnel'>
   <view class='iconfont icon-resume1 f24'></view>
    <view>我的简历</view>
  </view>
   <view class='menu_item f12 tc' bindtap='goFavorite'>
   <view class='iconfont icon-shoucang f24'></view>
    <view>我的收藏</view>
  </view>
  <view class='menu_item f12 tc' bindtap='goCircle'>
  <view class='iconfont icon-fabiaohuati f24'></view>
    <view>我的发表</view>
  </view>
   <view class='menu_item f12 tc' bindtap='goCarpool'>
  <view class='iconfont icon-maiche f24'></view>
    <view>我的拼车</view>
  </view>
</view>